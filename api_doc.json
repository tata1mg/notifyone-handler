                      "type": "object",
                      "properties": {
                        "url": {
                          "type": "string",
                          "example": "https:\/\/1mg-odin-production.s3.ap-south-1.amazonaws.com\/upload\/sales_orders\/42550349\/6f55151e-adb5-4171-8fe2-5eb6599eafb7.pdf",
